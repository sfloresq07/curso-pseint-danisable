Algoritmo llenar_matriz
	
	Dimension nums[3,3];
	
	cont <- 0;
	
	Para a<-1 Hasta 3 Con Paso 1 Hacer
		Para b<- 1 Hasta 3 Con Paso  1 Hacer
			cont <- cont + 1;
			nums[a,b] <- cont;
		FinPara
	FinPara
	
	Para c<-1 Hasta 3 Con Paso 1 Hacer
		Para d<- 1 Hasta 3 Con Paso  1 Hacer
			
			Escribir nums[c,d]
			
		FinPara
	FinPara
	

	
	
FinAlgoritmo
